<div class="page-header">
  <h1>All city programs</h1>
</div>

<p><a href="/all_city_programs/new" class="btn btn-primary">Add a New All City Program</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @all_city_programs.each do |all_city_program| %>
      <tr>
        <td>
          <a href="/all_city_programs/<%= all_city_program.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/all_city_programs/<%= all_city_program.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_all_city_program/<%= all_city_program.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

