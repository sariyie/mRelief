<div class="page-header">
  <h1>Medicaids</h1>
</div>

<p><a href="/medicaids/new" class="btn btn-primary">Add a New Medicaid</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Medicaid household size</th>
      <th>Medicaid gross income</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @medicaids.each do |medicaid| %>
      <tr>
        <td><%= medicaid.medicaid_household_size %></td>
        <td><%= medicaid.medicaid_gross_income %></td>
        <td>
          <a href="/medicaids/<%= medicaid.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/medicaids/<%= medicaid.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_medicaid/<%= medicaid.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

